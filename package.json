{
  "name": "@neuralegion/cypress-har-generator",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/cypress-har-generator.git"
  },
  "author": {
    "name": "Artem Derevnjuk",
    "email": "artem.derevnjuk@neuralegion.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NeuraLegion/cypress-har-generator/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "lint": "eslint -c .eslintrc 'src/**/*.ts'",
    "pretty": "prettier --check 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "homepage": "https://github.com/NeuraLegion/cypress-har-generator#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^23.3.1",
    "@types/node": "~10.12.18",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "cypress": "3.3.2",
    "devtools-protocol": "0.0.719330",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prefer-arrow": "^1.1.6",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.7",
    "jest": "^23.5.0",
    "lint-staged": "^9.4.1",
    "nyc": "~14.1.1",
    "prettier": "~1.19.1",
    "semantic-release": "^15.13.27",
    "ts-jest": "^23.1.3",
    "ts-node": "~8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.20.1",
    "tslint-plugin-prettier": "~2.0.1",
    "typescript": "~3.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "chrome-remote-interface": "^0.28.0"
  }
}
