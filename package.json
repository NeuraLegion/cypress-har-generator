{
  "name": "@neuralegion/cypress-har-generator",
  "version": "5.16.4",
  "private": false,
  "description": "The Cypress plugin for generating HTTP Archive (HAR) files is a tool that allows developers and QA engineers to capture detailed information about network requests made during the execution of Cypress tests.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/cypress-har-generator.git"
  },
  "type": "module",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js",
    "types": {
      "import": "./dist/mjs/index.d.ts",
      "require": "./dist/cjs/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "typings": "./dist/cjs/index.d.ts",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist",
    "commands.js",
    "commands.js.map"
  ],
  "author": {
    "name": "Artem Derevnjuk",
    "email": "artem.derevnjuk@neuralegion.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NeuraLegion/cypress-har-generator/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cypress",
    "har",
    "electron",
    "cypress-plugin",
    "chrome",
    "http-archive",
    "testing",
    "qa",
    "automation",
    "typescript",
    "chromium",
    "qatools"
  ],
  "scripts": {
    "semantic-release": "semantic-release",
    "lint": "eslint -c eslint.config.js 'src/**/*.ts'",
    "format": "prettier --check 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "start": "node -r ts-node/register cypress/app --port 8080",
    "e2e": "cypress run --browser chrome --headless",
    "build": "webpack --config webpack.config.ts",
    "prepublishOnly": "npm run build",
    "prepare": "is-ci || husky install"
  },
  "homepage": "https://github.com/NeuraLegion/cypress-har-generator#readme",
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@eslint/eslintrc": "^3.0.0",
    "@jest/globals": "^29.7.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai-like": "^1.1.3",
    "@types/chai-things": "^0.0.38",
    "@types/chalk": "^2.2.0",
    "@types/eslint": "^8.56.2",
    "@types/express": "^4.17.21",
    "@types/har-format": "^1.2.15",
    "@types/jest": "^29.5.11",
    "@types/node": "~20.11.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "cypress": "^13.6.3",
    "devtools-protocol": "~0.0.1250650",
    "esbuild": "0.19.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "express": "^4.18.2",
    "globals": "^13.24.0",
    "hbs": "^4.2.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "minimist": "^1.2.8",
    "prettier": "~3.2.4",
    "semantic-release": "^23.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "~10.9.2",
    "typescript": "~5.3.3",
    "ws": "^8.16.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.{ts,js,json,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "cypress": ">=4.4.1 <12.12.0 || >=12.17.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chrome-remote-interface": "~0.33.0",
    "tslib": "^2.6.2"
  }
}
